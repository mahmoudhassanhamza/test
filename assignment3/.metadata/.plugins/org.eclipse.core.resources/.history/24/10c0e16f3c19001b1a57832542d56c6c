#include "stdio.h"

void Transpose()
{
	int a[100][100],b[100][100];
	int i,j,rows,col;
	printf("enter number of rows of matrix\n");
	scanf("%d",&rows);
	fflush(stdout);
	printf("enter number of rows of matrix\n");
	scanf("%d",&col);
	fflush(stdout);
	for (i=0;i<rows;i++)
		for (j=0;j<col;j++)
		{
			printf("a[%d][%d] = ",i,j);
			fflush(stdout);
			scanf("%d",&a[i][j]);
			fflush(stdin);
		}

	for (i=0;i<rows;i++)
		for (j=0;j<col;j++)
		{
			printf("a[%d][%d] = ",i,j);
			fflush(stdout);
			scanf("%d",&a[i][j]);
			fflush(stdin);
		}


}
