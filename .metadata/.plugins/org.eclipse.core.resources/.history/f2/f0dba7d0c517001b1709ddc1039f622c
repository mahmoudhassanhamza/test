#include "stdio.h"
#include "headers.h"

int main()
{
	int x,y;
	printf("enter a positive number");
	fflush(stdout);
	scanf("%d",&x);
	fflush(stdin);
	if(x<0)
	{
		printf("enter a positive number");
		fflush(stdout);
		return 0;
	}
	y = Factorial(x);
	printf("Factorial %d is %d",x,y);
	return 0;

}
