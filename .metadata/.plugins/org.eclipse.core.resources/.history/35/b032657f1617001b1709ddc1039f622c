#include "stdio.h"

int main(){

	char x;
	fflush(stdin);
	printf("enter a character \n");
	fflush(stdout);
	scanf("%d",&x);
	fflush(stdin);

	switch(x){
	case 'a':
	case 'e':
	case 'i':
	case 'o':
	case 'u':
	{printf("%c is a vowel",x);}
	default:
		printf("%c is a consonant",x);

	}

	return 0;

}
